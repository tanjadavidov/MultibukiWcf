<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of 
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator 
    finds an attribute "name" that has a value of "MyDB".
    
    <connectionStrings>
      <add name="MyDB" 
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True" 
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->
  <connectionStrings>
    <!--<add name="BioEnConnectionString"
        connectionString="Data Source=APP016\MSSQLSERVER2017;Initial Catalog=BioEn;Persist Security Info=True;User ID=bioen.admin;Password=bioen2019" providerName="System.Data.SqlClient"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>-->

    <!-- ovaj deo: xdt:Transform="SetAttributes" xdt:Locator="Match(name)" se dodaje u connectionStringu zbog transformacije-da odavde čita conekcioni string-->
    <!--connectionString="Data Source=ACER\MSSQLSERVER2017;Initial Catalog=Multibuki;Persist Security Info=True;User ID=multibuki.admin; Password=multibuki2020" providerName="System.Data.SqlClient"-->

    <add name="MultiBukiConnectionString"       
         connectionString="Data Source=APP016\MSSQLSERVER2017;Initial Catalog=Multibuki;Persist Security Info=True;User ID=multibuki.admin; Password=multibuki2020" providerName="System.Data.SqlClient"         
         xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <system.web>
    
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <!--<compilation debug="true" targetFramework="4.7.2" xdt:Transform="Replace"/>-->
    <!--
      In the example below, the "Replace" transform will replace the entire 
      <customErrors> section of your web.config file.
      Note that because there is only one customErrors section under the 
      <system.web> node, there is no need to use the "xdt:Locator" attribute.
      
      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
</configuration>